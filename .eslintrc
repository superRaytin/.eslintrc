{
  "rules": {
    "strict": [2, "global"],
    "space-before-blocks": 2,
    "space-after-keywords": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-empty": 2,
    "no-with": 2,
    "no-extra-parens": 2,
    "no-lone-blocks": 2,
    "no-invalid-regexp": 2,
    "no-eval": 2,
    "no-debugger": 2,
    "no-alert": 2,
    "no-warning-comments": [0, {
      "terms": ["todo", "fixme", "xxx"],
      "location": "start"
    }],
    "no-console": 0,
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "no-spaced-func": 2,
    "newline-after-var": 0,
    "curly": [2, "all"],
    "eqeqeq": 2,
    "comma-spacing": 2,
    "no-trailing-spaces": 2,
    "spaced-comment": 2,
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }],
    "quotes": [2, "single"],
    "brace-style": [0, "1tbs"],
    "no-new-func": 2,
    "no-new-object": 2,
    "space-before-function-paren": [1, "never"],
    "no-irregular-whitespace": 2
  },
  "env": {
    "amd": true,
    "jasmine": false,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true,
    "es6": true
  },
  "extends": "eslint:recommended"
}
